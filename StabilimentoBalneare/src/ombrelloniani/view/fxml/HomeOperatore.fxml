<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<GridPane styleClass="container" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="ombrelloniani.view.fxmlControllers.HomeOperatoreEventsController">
	<GridPane>
		<VBox styleClass="menuHome" GridPane.columnIndex="1">
			<AnchorPane>
				<Label styleClass="title" text="HOME OPERATORE" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="0.0"
					AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0" />
			</AnchorPane>
			<VBox styleClass="menuButtonArea" VBox.vgrow="ALWAYS">
				<Button fx:id="gestionePren" onAction="#handleGestionePrenButtonClick" onMouseEntered="#handleHoverInButton"
					onMouseExited="#handleHoverOutButton" styleClass="button, mainButton, menuButton" text="Gestione Prenotazione" />
				<Button fx:id="controlloDisp" onAction="#handleGestionePrenButtonClick" onMouseEntered="#handleHoverInButton"
					onMouseExited="#handleHoverOutButton" styleClass="button, mainButton, menuButton" text="Controllo DisponibilitÃ " />
			</VBox>
		</VBox>
		<rowConstraints>
			<RowConstraints percentHeight="100.0" />
		</rowConstraints>
		<columnConstraints>
			<ColumnConstraints percentWidth="35.0" />
			<ColumnConstraints percentWidth="50.0" />
			<ColumnConstraints percentWidth="15.0" />
		</columnConstraints>
	</GridPane>
	<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" GridPane.halignment="RIGHT" GridPane.valignment="TOP">
		<Button fx:id="logoutButton" onAction="#handleLogoutButtonClick" styleClass="button, logoutButton"
			text="Logout" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0" />
	</AnchorPane>
	<rowConstraints>
		<RowConstraints percentHeight="100.0" />
	</rowConstraints>
	<columnConstraints>
		<ColumnConstraints percentWidth="100.0" />
	</columnConstraints>
</GridPane>
